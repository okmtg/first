import csv
file="IQ_level.csv"
dic={}

with open(file, newline="") as csvfile:
    reader = csv.DictReader(csvfile)
    for row in reader:
        dic[row["country"]] = {
            "rank" : row['ï»¿rank'],
            "iq" : row['ï»¿IQ'],
            "education": row['ï»¿education'],
            "income": row['ï»¿avg_income'],
            "temp": row['ï»¿avg_temp']
            }
        
